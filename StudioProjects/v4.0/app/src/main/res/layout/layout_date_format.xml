                        android:layout_weight="1"
                        android:text="@string/p_dateformat_DAY_MM_DD_YYYY" />

                    <RadioButton
                        android:id="@+id/radioButton22"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/p_dateformat_DAY_MM_DD_YY" />

                    <RadioButton
                        android:id="@+id/radioButton23"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/p_dateformat_DAY_DD_MM_YYYY" />

                    <RadioButton
                        android:id="@+id/radioButton24"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/p_dateformat_DAY_DD_MM_YY" />

                    <RadioButton
                        android:id="@+id/radioButton25"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/p_dateformat_DY_MMMMDDYYYY" />

                    <RadioButton
